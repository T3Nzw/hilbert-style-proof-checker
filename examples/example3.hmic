theorem Identity := 
  assume.
  goal A -> A.
  begin.
    A -> A -> A by AX K.
    A -> (A -> A) -> A by AX K.
    (A -> (A -> A) -> A) -> (A -> A -> A) -> A -> A by AX S.
    (A -> A -> A) -> A -> A by MP.
    A -> A by MP.
  qed.

theorem ProveIdentityUsingIdentity :=
  assume.
  goal A -> A -> A.
  begin.  
    A -> A by TH Identity.
  qed.
